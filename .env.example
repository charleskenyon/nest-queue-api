PORT=3001
NODE_ENV=development

# Queue Provider: sqs or rabbitmq
QUEUE_PROVIDER=sqs

# AWS SQS Config (LocalStack)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_ENDPOINT=http://localhost:4566
SQS_QUEUE_URL=http://localhost:4566/000000000000/message-queue

# RabbitMQ Config
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_QUEUE_NAME=message-queue
RABBITMQ_EXCHANGE=message-exchange
RABBITMQ_ROUTING_KEY=messages